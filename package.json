{
  "name": "swipestats-neon-prisma",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "env:dev": "cp .env.dev .env",
    "env:staging": "cp .env.staging .env",
    "env:prod": "cp .env.prod .env",
    "build": "next build",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:up": "bun run env:dev && bun run db:docker && bun run db:migrate-reset && bun run dev",
    "db:docker": "docker kill swipestats-postgres || true && docker run --rm -P -p 127.0.0.1:5432:5432 -e POSTGRES_PASSWORD=mysecretpassword -e POSTGRES_USER=user -e POSTGRES_DB=swipestats --name swipestats-postgres -d postgres:alpine",
    "db:migrate-reset": "dotenv tsx scripts/migrate-reset.ts",
    "db:migrate-dev": "dotenv tsx scripts/migrate-dev.ts",
    "db:migrate-dev-draft": "prisma migrate dev --create-only",
    "db:migrate-deploy": "tsx scripts/migrate-deploy.ts",
    "db:migrate-rollback": "prisma migrate resolve --rolled-back",
    "db:migrate-reset-migrations": "rm -rf prisma/migrations && bun run db:migrate-reset && bun run db:migrate-dev && bun run dev",
    "dev": "next dev",
    "email:dev": "email dev -d src/emails -p 2000",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "slicemachine": "start-slicemachine",
    "reset-cache": "rm -rf .next && rm -rf node_modules && rm -rf package-lock.json && bun install"
  },
  "dependencies": {
